{
    "manifest_version": 3,
    "name": "Decriptador de Permiss√µes - Painel",
    "version": "1.0",
    "permissions": [
      "storage",
      "activeTab",
      "scripting"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
      }
    },
    "content_scripts": [
      {
        "matches": ["https://hm-painel.metodologiagb.com.br/*"],
        "js": [
            "crypto-js.min.js",
            "contentScript.js",
            "crypto-helper.js"
          ]
      }
    ],
    "host_permissions": [
      "https://hm-painel.metodologiagb.com.br/*"
    ]
  }